{
  "sap.app": {
    "_version": "1.1.0",
    "id": "iService_UI5",
    "type": "application",
    "applicationVersion": {
      "version": "1.1.0"
    },
    "title": "Main Monitor"
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "iService_UI5.view.main",
      "type": "XML",
      "async": true
    },
    "models": {
      "Settings": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/applicationProperties.json",
        "preload": true
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "iService_UI5.i18n.i18n"
        }
      },
      "expertCreate": {
          "type": "sap.ui.model.json.JSONModel",
          "uri": "model/expertCreate.json",
          "preload": true,
          "settings": {}
      },
      "labelCreate": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/labelCreate.json",
        "preload":true,
        "settings":{}
      }
    },
    "routing": {
      "config": {
        "viewPath": "iService_UI5.view",
        "controlAggregation": "pages",
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "transition": "slide",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "app",
          "target": "app"
        },
        {
          "pattern": "expert",
          "name": "expert",
          "target": "expert"
        },
        {
          "pattern": "createExpert",
          "name": "createExpert",
          "target": "createExpert"
        },
        {
          "pattern": "label",
          "name": "label",
          "target":"label"
        },
        {
          "pattern": "createLabel",
          "name":"createLabel",
          "target":"createLabel"
        }
        
      
      ],
      "targets": {
        "app": {
          "viewId": "app",
          "viewName": "main",
          "controlId": "app",
          "viewLevel": 1
        },
        "expert": {
          "viewId": "expert",
          "viewName": "expert",
          "controlId": "app",
          "viewLevel": 1
        },
        "createExpert": {
          "viewId": "createExpert",
          "viewName": "createExpert",
          "controlId": "app",
          "viewLevel": 2
        },
        "label": {
          "viewId": "label",
          "viewName": "label",
          "controlId": "app",
          "viewLevel": 1
        },
        "createLabel": {
            "viewId": "createLabel",
            "viewName": "createLabel",
            "controlId": "app",
            "viewLevel": 2
        }
      }
    }
  }
}